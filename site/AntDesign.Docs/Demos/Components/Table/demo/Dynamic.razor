@using System.ComponentModel
@using AntDesign.TableModels

<Table TItem="Dictionary<string, object>" DataSource="@data" Loading="data==null">
	@if (data?.Any() == true)
	{
		@foreach (var key in data.FirstOrDefault()?.Keys)
		{
			<Column TData="object" DataIndex=@($"['{key}']") Title="@key"></Column>
		}
	}
</Table>

@inject HttpClient httpClient;
@code {

	List<Dictionary<string, object>> data;

	protected override async Task OnInitializedAsync()
	{
		data = await httpClient.GetFromJsonAsync<List<Dictionary<string, object>>>("https://api.github.com/repos/ant-design-blazor/ant-design-blazor/contributors?per_page=200");
	}
}
