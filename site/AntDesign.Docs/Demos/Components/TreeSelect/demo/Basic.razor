@using AntDesign.Docs.Demos.Components.TreeSelect.Demo 


<TreeSelect TItem="WebMenu" TItemValue="string" @bind-Value="selectMenuId"
            DataSource="@(LocalJsonData.GetMenusByParentId("0"))"
            TitleExpression="m => m.DataItem.MenuName" KeyExpression="m => m.DataItem.MenuId"
            ChildrenExpression="m => LocalJsonData.GetMenusByParentId(m.DataItem.MenuId)" 
            IsLeafExpression="m => LocalJsonData.HasChild(m.DataItem.MenuId)" 
            >
</TreeSelect>

select MenuId: @selectMenuId

@code{
    private string selectMenuId;
}