@namespace AntDesign
@inherits AntDomComponentBase

@{
    var coinfigs = _configDict.Values.OrderBy(x=>x.Order).ToList();
    if (coinfigs.Count > 0)
    {
        <div>
            <div class="@ClassMapper.Class" style="top: @(_top)px;" @ref="Ref">
                <div class="ant-message-notice-wrapper">
                    @foreach (var config in coinfigs)
                    {
                        <MessageItem @key="config" Config="config" OnMouseEnter="OnMouseEnter" OnMouseLeave="OnMouseLeave"></MessageItem>
                    }
                </div>
            </div>
        </div>
    }
}

@_styleContent