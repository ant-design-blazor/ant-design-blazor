@namespace AntDesign
@inherits AntDomComponentBase

<footer class="@ClassMapper.Class" @ref="Ref" style="@Style" id="@Id">
    @ChildContent
</footer>

@code {
    /// <summary>
    /// The inner content
    /// </summary>
    [Parameter]
    public RenderFragment ChildContent { get; set; }

    protected override void OnInitialized()
    {
        ClassMapper.Add("ant-layout-footer");

        base.OnInitialized();
    }
}
