@namespace AntDesign
@inherits AntDomComponentBase

@if (_hasAdd)
{
    <CascadingValue Value="@this.Id" Name="DialogWrapperId" IsFixed>
        <Dialog @ref="_dialog"
                Config="Config"
                Visible="@Visible"
                Style="@Style"
                Class="@Class"
                Id="@Id"
                >
            @ChildContent
        </Dialog>
    </CascadingValue>
}
